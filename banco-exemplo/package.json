{
   "name": "banco-exemplo",
   "version": "1.0.0",
   "main": "index.js",
   "license": "MIT",
   "scripts": {
      "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
      "test:verbose": "jest --passWithNoTests --runInBand --watchAll",
      "coverage": "jest --coverage",
      "dev": "tsx watch src/main/server.ts --ignore node_modules",
      "lint": "eslint . --ext .ts --fix",
      "build": "tsc --project tsconfig.build.json && tscpaths -p tsconfig .json -s ./src -o ./dist",
      "start": "ts-node src/index.ts",
      "migration": "tsx knex migrate:up"
   },
   "dependencies": {
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "knex": "^3.0.1",
      "pg": "^8.11.3",
      "swagger-ui-express": "^5.0.0",
      "tsconfig-paths": "^4.2.0"
   },
   "devDependencies": {
      "@types/jest": "^29.5.5",
      "@types/node": "^16.11.10",
      "@typescript-eslint/eslint-plugin": "^6.8.0",
      "@typescript-eslint/parser": "^6.8.0",
      "eslint": "^8.51.0",
      "jest": "^29.7.0",
      "jest-mock-extended": "^3.0.5",
      "sqlite3": "^5.1.6",
      "ts-jest": "^29.1.1",
      "tsx": "^3.14.0",
      "typescript": "4.5.2"
   }
}
